<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.ToyRobotTV" name="Toy Robot TV" version="0.2" provider-name="ApplePay">
    <requires>
    </requires>
    <extension point="xbmc.python.pluginsource"
               library="default.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <platform>all</platform>
        <summary>Addon para ver Series de TV buscanos en https://www.facebook.com/ToyRobotTV/</summary>
        <description>Addon para ver Series de TVbuscanos en https://www.facebook.com/ToyRobotTV/ </description>
    </extension>
</addon>


<addon id="repository.ToyRobot-TV" name="Toy Robot TV  Repo" version="0.0.3" provider-name="Toy Robot TV">
	<extension point="xbmc.addon.repository" name="Toy Robot TV Official Repo">
		<info compressed="false">https://raw.github.com/albertquest/Toy-Robot-TV-Rep/master/addons.xml</info>
		<checksum>https://raw.github.com/albertquest/Toy-Robot-TV-Rep/master/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.github.com/albertquest/Toy-Robot-TV-Rep/master</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Toy Robot TV Official Repo</summary>
   		<description lang="en">Instala los Addons de Toy Robot TV.</description>
		<disclaimer></disclaimer>
		<platform>all</platform>
	</extension>
</addon>
</addons>
